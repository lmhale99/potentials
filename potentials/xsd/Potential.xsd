    <xsd:complexType name="note-type">
        <xsd:sequence>
            <xsd:element form="unqualified" minOccurs="0" name="title" type="xsd:string"/>
            <xsd:element form="unqualified" minOccurs="0" name="date" type="xsd:date"/>
            <xsd:element form="unqualified" minOccurs="0" name="text" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="citation-type">
        <xsd:sequence>
            <xsd:element form="unqualified" minOccurs="0" name="document-type">
                <xsd:annotation>
                    <xsd:documentation>The type of publication</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="book"/>
                        <xsd:enumeration value="journal"/>
                        <xsd:enumeration value="report"/>
                        <xsd:enumeration value="patent"/>
                        <xsd:enumeration value="thesis"/>
                        <xsd:enumeration value="conference proceedings"/>
                        <xsd:enumeration value="archived document"/>
                        <xsd:enumeration value="personal correspondence"/>
                        <xsd:enumeration value="published translation"/>
                        <xsd:enumeration value="unspecified"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="0" name="title" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>Title of the publication</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element form="unqualified" maxOccurs="unbounded" minOccurs="0" name="author">
                <xsd:annotation>
                    <xsd:documentation>Author of publication</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element form="unqualified" minOccurs="0" name="given-name" type="xsd:string"/>
                        <xsd:element form="unqualified" minOccurs="0" name="surname" type="xsd:string"/>
                        <xsd:element form="unqualified" minOccurs="0" name="suffix" type="xsd:string"/>
                        <xsd:element form="unqualified" maxOccurs="unbounded" minOccurs="0" name="affiliation" type="xsd:string"/>
                        <xsd:element form="unqualified" minOccurs="0" name="ORCID" type="xsd:string"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="0" name="publication-name" type="xsd:string"/>
            <xsd:element form="unqualified" minOccurs="0" name="publication-date">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element form="unqualified" minOccurs="0" name="year" type="xsd:gYear"/>
                        <xsd:element form="unqualified" minOccurs="0" name="month" type="xsd:gMonth"/>
                        <xsd:element form="unqualified" minOccurs="0" name="day" type="xsd:gDay"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="0" name="volume" type="xsd:string"/>
            <xsd:element form="unqualified" minOccurs="0" name="issue" type="xsd:string"/>
            <xsd:element form="unqualified" minOccurs="0" name="abstract" type="xsd:string"/>
            <xsd:element form="unqualified" minOccurs="0" name="pages" type="xsd:string"/>
            <xsd:element form="unqualified" minOccurs="0" name="DOI" type="xsd:string"/>
            <xsd:element form="unqualified" minOccurs="0" name="URL" type="xsd:anyURI"/>
            <xsd:element form="unqualified" minOccurs="0" name="bibtex" type="xsd:string"/>
            <xsd:element form="unqualified" minOccurs="0" name="text-bibliography" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="interatomic-potential-type">
        <xsd:sequence>
            <xsd:element form="qualified" name="key" type="xsd:string"/>
            <xsd:element form="qualified" name="id" type="xsd:string"/>
            <xsd:element form="qualified" name="URL" minOccurs="0" maxOccurs="1" type="xsd:string" />
            <xsd:element form="qualified" minOccurs="0" name="record-version" type="xsd:string"/>
            <xsd:element form="qualified" minOccurs="0" name="description">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="local" type="note-type"> </xsd:element>
                        <xsd:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="citation" type="citation-type"> </xsd:element>
                        <xsd:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="notes" type="note-type"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="keyword" type="xsd:string"/>
            <xsd:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="implementation">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element form="qualified" minOccurs="0" name="key" type="xsd:string"/>
                        <xsd:element form="qualified" minOccurs="0" name="id" type="xsd:string"/>
                        <xsd:element form="qualified" minOccurs="0" name="status" type="xsd:string"/>
                        <xsd:element form="qualified" minOccurs="0" name="date" type="xsd:date"/>
                        <xsd:element form="qualified" minOccurs="0" name="type" type="xsd:string"/>
                        <xsd:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="notes" type="note-type"/>
                        <xsd:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="artifact">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element form="qualified" minOccurs="0" name="key" type="xsd:string"/>
                                    <xsd:element form="qualified" minOccurs="0" name="id" type="xsd:string"/>
                                    <xsd:element form="qualified" minOccurs="0" name="status" type="xsd:string"/>
                                    <xsd:element form="qualified" minOccurs="0" name="type" type="xsd:string"/>
                                    <xsd:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="notes" type="note-type"/>
                                    <xsd:element form="qualified" minOccurs="0" name="web-link">
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element form="qualified" name="URL" type="xsd:anyURI"/>
                                                <xsd:element form="qualified" minOccurs="0" name="label" type="xsd:string"/>
                                                <xsd:element form="qualified" minOccurs="0" name="link-text" type="xsd:string"/>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="link">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element form="qualified" minOccurs="0" name="key" type="xsd:string"/>
                                    <xsd:element form="qualified" minOccurs="0" name="id" type="xsd:string"/>
                                    <xsd:element form="qualified" minOccurs="0" name="status" type="xsd:string"/>
                                    <xsd:element form="qualified" minOccurs="0" name="type" type="xsd:string"/>
                                    <xsd:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="notes" type="note-type"/>
                                    <xsd:element form="qualified" minOccurs="0" name="web-link">
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element form="qualified" name="URL" type="xsd:anyURI"/>
                                                <xsd:element form="qualified" minOccurs="0" name="label" type="xsd:string"/>
                                                <xsd:element form="qualified" minOccurs="0" name="link-text" type="xsd:string"/>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="parameter" type="parameter-type"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="element" type="chemical-element-type"/>
            <xsd:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="fictional-element" type="chemical-element-type"/>
            <xsd:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="other-element" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="parameter-type">
        <xsd:sequence>
            <xsd:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="value" type="xsd:float"/>
            <xsd:element form="qualified" minOccurs="0" name="unit" type="xsd:string"/>
            <xsd:element form="qualified" minOccurs="0" name="name" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="interatomic-potential" type="interatomic-potential-type"> </xsd:element>
</xsd:schema>