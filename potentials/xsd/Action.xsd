                            <xsd:enumeration value="updated posting"/>
                            <xsd:enumeration value="retraction"/>
                            <xsd:enumeration value="site change"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>
                <xsd:element maxOccurs="unbounded" minOccurs="0" name="potential" type="interatomic-potential-compact-id-type"/>
                <xsd:element minOccurs="0" name="comment" type="xsd:string"/>
            </xsd:sequence>
        </xsd:complexType>
