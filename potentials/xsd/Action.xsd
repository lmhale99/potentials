    <xsd:complexType name="interatomic-potential-compact-id-type">
        <xsd:sequence>
            <xsd:element form="unqualified" minOccurs="0" name="key" type="xsd:string"/>
            <xsd:element form="unqualified" minOccurs="0" name="id" type="xsd:string"/>
            <xsd:element form="unqualified" minOccurs="0" name="implementation">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element form="unqualified" minOccurs="0" name="key" type="xsd:string"/>
                        <xsd:element form="unqualified" minOccurs="0" name="id" type="xsd:string"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element form="unqualified" maxOccurs="unbounded" minOccurs="0" name="artifact">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element form="unqualified" minOccurs="0" name="key" type="xsd:string"/>
                        <xsd:element form="unqualified" minOccurs="0" name="id" type="xsd:string"/>
                        <xsd:element form="unqualified" minOccurs="0" name="web-link">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element form="unqualified" name="URL" type="xsd:anyURI"/>
                                    <xsd:element form="unqualified" minOccurs="0" name="label" type="xsd:string"/>
                                    <xsd:element form="unqualified" minOccurs="0" name="link-text" type="xsd:string"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="0" name="doi" type="xsd:string"/>
            <xsd:element form="unqualified" maxOccurs="unbounded" minOccurs="0" name="element" type="chemical-element-type"/>
            <xsd:element form="unqualified" maxOccurs="unbounded" minOccurs="0" name="fictional-element" type="chemical-element-type"/>
            <xsd:element form="unqualified" maxOccurs="unbounded" minOccurs="0" name="other-element" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="action">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="date" type="xsd:date">
                    <xsd:annotation>
                        <xsd:appinfo>
                            <placeholder>YYYY-MM-DD</placeholder>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="type">
